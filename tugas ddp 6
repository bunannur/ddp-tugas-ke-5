# Meminta input jumlah baris
jumlah_baris = int(input("Masukkan jumlah baris: "))

# Menggunakan for loop untuk mencetak pola
for baris in range(1, jumlah_baris +1):
    for kolom in range(baris):
        print("*", end="")
    print()  # Untuk mengakhiri baris dan melanjutkan ke barisÂ berikutnya
